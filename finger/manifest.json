{
  "name": "Finger",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Use your finger to do great things.",
  "icons": { "16": "icon.png" },
  "permissions": [
    "tabs"
  ],
  "background": {
    "scripts": ["background.js"]
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": ["http://localhost:8000/camera.html"],
      "css": ["mystyles.css"],
      "js": ["jquery.js", "myscript.js"]
    }
  ],
}
